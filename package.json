{
    "name": "simplebrowser-helper-extension",
    "publisher": "codevoid",
    "displayName": "SimpleBrowser Helper",
    "description": "Open a simple browser to a preconfigured URL on demand, or startup",
    "repository": {
        "type": "git",
        "url": "https://github.com/grork/SimpleBrowseHelper"
    },
    "bugs": {
        "url": "https://github.com/grork/SimpleBrowserHelper/issues/"
    },
    "homepage": "https://github.com/grork/SimpleBrowserHelper/README.md",
    "version": "0.0.1",
    "extensionKind": [
        "ui"
    ],
    "capabilities": {
        "untrustedWorkspaces": {
            "supported": true
        }
    },
    "engines": {
        "vscode": "^1.66.0"
    },
    "categories": [
        "Other"
    ],
    "keywords": [
        "browser",
        "preview"
    ],
    "activationEvents": [
        "onCommand:simplebrowser-helper-extension.openDefaultUrl",
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [
            {
                "command": "simplebrowser-helper-extension.openDefaultUrl",
                "title": "Open Default URL",
                "category": "Simple Browser Helper"
            }
        ],
        "configuration": [
            {
                "title": "Simple Browser Helper",
                "properties": {
                    "codevoid.simplebrowser-helper-extension.defaultUrl": {
                        "title": "URL to open the browser too",
                        "type": "string",
                        "default": null
                    }
                }
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^7.2.0",
        "@types/mocha": "^9.1.0",
        "@types/node": "14.x",
        "@types/node-fetch": "^2.6.1",
        "@types/vscode": "^1.66.0",
        "@vscode/test-electron": "^2.1.3",
        "glob": "^7.2.0",
        "mocha": "^9.2.2",
        "typescript": "^4.5.5"
    },
    "dependencies": {
        "node-fetch": "^2.6.7"
    }
}