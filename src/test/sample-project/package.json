{
  "name": "codevoid.simplebrowser.test",
  "version": "0.0.1",
  "description": "Sample Workspace for Simple Browser Tests",
  "scripts": {
    "build": "copyfiles --exclude \"node_modules/**\" --exclude \"output/**\" \"**/*.html\" output",
    "serve": "browser-sync start --server output --watch --no-ui --listen localhost --no-online --no-open --reload-debounce 250"
  },
  "author": "Dominic Hopton",
  "license": "MIT",
  "devDependencies": {
    "browser-sync": "^2.27.9",
    "copyfiles": "^2.4.1"
  }
}